    "@types/cors": "^2.8.12",
    "@types/cron": "^2.0.0",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/http-errors": "^1.8.2",
