{
  "name": "pasivo",
  "version": "1.0.0",
  "description": "APIS PASIVO",
  "main": "index.js",
  "scripts": {
    "start:prod": "npm run build && cross-env NODE_ENV=production REDIS=redis:6379 node dist/server.js",
    "start:dev": "npm run build && cross-env NODE_ENV=development  REDIS=redis:6379 node dist/server.js",
    "start:local": "npm run build && cross-env NODE_ENV=development  REDIS=localhost:6379 node dist/server.js",
    "dev": "cross-env NODE_ENV=development REDIS=redis:6379 nodemon",
    "local": "cross-env NODE_ENV=development REDIS=localhost:6379 nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "test:dev": "cross-env NODE_ENV=development  REDIS=redis:6379 jest  --detectOpenHandles --forceExit",
    "test:local": "cross-env NODE_ENV=development  REDIS=localhost:6379 jest  --detectOpenHandles --forceExit",
    "test:prod": "cross-env NODE_ENV=production jest --forceExit --detectOpenHandles ",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -rf dist && rm -rf src/logs",
    "postinstall": "npm run build",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only pasivo-api"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
